package dto;

import java.util.Date;

public abstract class EmpleadoAbs extends Persona {
	
	private double salario;
	private Date inicioLaboral;
	
	private EmpleadoAbs(String id, String nombre, String apellido, Date fecha, double salario, Date inicioLaboral) {
		super(id, nombre, apellido, fecha);
		this.setSalario(salario);
		this.inicioLaboral = inicioLaboral;
	}

	public double getSalario() {
		return salario;
	}

	public void setSalario(double salario) {
		this.salario = salario;
	}
	
	

}
